<div class="container jumbotron">
<h1>Register</h1>
</div>

<%= render "partials/flash" %>

<%= form_for @user, url: users_path do |f| %>
  <%= f.personal_id_field :personal_id, placeholder: "Enter your NRIC/FIN" %>
  <%= f.name_field :name, placeholder: "Enter your name" %>
  <%= f.email_field :email, placeholder: "Enter your email" %>
  <%= f.password_field :password, placeholder: "Enter your password" %>
  <%= f.contact_no_field :contact_no, placeholder: "Enter your contact number" %>

  <%= f.submit "Register" %>
<% end %>
