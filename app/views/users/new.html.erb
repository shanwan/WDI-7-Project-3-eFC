<div class="container jumbotron">
<h1>Register</h1>
</div>

<%= render "partials/flash" %>

<%= form_for @user, url: users_path do |f| %>
  <%= f.text_field :personal_id, placeholder: "Enter your personal_id/FIN" %>
  <%= f.text_field :name, placeholder: "Enter your name" %>
  <%= f.text_field :email, placeholder: "Enter your email" %>
  <%= f.text_field :password, placeholder: "Enter your password" %>
  <%= f.text_field :contact_no, placeholder: "Enter your contact number" %>

  <%= f.submit "Register" %>
<% end %>
